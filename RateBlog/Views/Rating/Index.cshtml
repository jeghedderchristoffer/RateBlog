@model RateBlog.Models.RatingViewModels.IndexViewModel

<div class="navbar-allaround">
</div>


<div class="outer">
    <div class="middle">
        <div class="inner">
            <h1>Giv feedback til Christoffer Jespersen</h1>

        </div>
    </div>
</div>
<br />



<div class="container">
    <div class="col-md-12 rating-box" id="orginalitetBox">
        <div class="col-md-4">
            <h4>Orginalitet</h4>
        </div>
        <div class="col-md-6">
            <fieldset class="rating center-block" id="orginalitetCheckBox">
                <input type="radio" id="field1_star5" name="rating1" value="5" /><label class="full" for="field1_star5"></label>

                <input type="radio" id="field1_star4" name="rating1" value="4" /><label class="full" for="field1_star4"></label>

                <input type="radio" id="field1_star3" name="rating1" value="3" /><label class="full" for="field1_star3"></label>

                <input type="radio" id="field1_star2" name="rating1" value="2" /><label class="full" for="field1_star2"></label>

                <input type="radio" id="field1_star1" name="rating1" value="1" /><label class="full" for="field1_star1"></label>

            </fieldset>

            <i class="fa fa-check fa-3x pull-right" id="orginalitetCheck" aria-hidden="true" style="display: none"></i>

        </div>
        <div class="col-md-2">
            <a class="pull-right" title="Orginalitet" data-content="Er denne influenter orginal? Er de ting, som denne person viser sine egne?" data-toggle="popover" data-trigger="hover" data-placement="left">
                <i class="fa fa-info-circle fa-2x" aria-hidden="true" style="color: #08468d;"></i>
            </a>
        </div>
    </div>

