    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: '326842804433660',
                cookie: true,
                xfbml: true,
                version: 'v2.8'
            });
            FB.AppEvents.logPageView();
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

    <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.css" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.js"></script>
    <script>
        window.addEventListener("load", function () {
            window.cookieconsent.initialise({
                "palette": {
                    "popup": {
                        "background": "#089de3",
                        "text": "#ffffff"
                    },
                    "button": {
                        "background": "#ffa500",
                        "text": "#ffffff"
                    }
                },
                "theme": "classic",
                "position": "bottom-left",
                "content": {
                    "message": "Vi bruger cookies for at forbedre din brugeroplevelse. Ved fortsat brug af websitet, acceptere du vores brug af cookies",
                    "link": "Læs mere her",
                    "href": "http://www.bestfluence.dk/Home/Privacy"
                }
            })
        });
    </script>

    <environment names="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        @*<link rel="stylesheet" href="~/css/site.css" />*@
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        @*<link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />*@
    </environment>
    @Html.Raw(JavaScriptSnippet.FullScript)

    <link rel="stylesheet" href="~/css/beststyle.css" asp-append-version="true" />
    <link href="https://fonts.googleapis.com/css?family=Anton|Oxygen|Raleway:300" rel="stylesheet">
    <link rel="stylesheet" href="~/css/social-buttons.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/lib/ihover/src/ihover.min.css">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

</head>
<body>
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MKL7H8F"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->

    <nav class="navbar navbar-default navbar-fixed-top" id="navigationBar">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand" id="brand"> <img id="navbarLogo" src="~/images/bestfluence-logo.svg" /><div>Bestfluence</div></a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li><a asp-action="Index" asp-controller="Influencer" class="navLinks">Find influencere</a></li>
            </ul>
            @await Html.PartialAsync("_LoginPartial")
        </div>
    </nav>

    <script>
        ga('set', 'userId', document.getElementById("applicationUserId").value);
    </script>

    <!--Change color of links-->

    @*<script type="text/javascript">
        if (location.pathname == "/" || window.location.href.indexOf("home") > -1) {
            var elements = document.getElementsByClassName("navLinks");
            for (i = 0; i < elements.length; i++) {
                elements[i].style.color = "#fff";
            }
            if (window.innerWidth > 767) {
                document.getElementById("navbarLogo").src = "../images/bestfluence-logo-white.svg";
            }
        }
    </script>*@

    <!-- Login Modal -->

    <div id="loginModal" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width: 400px;">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h3 class="modal-title text-center">Velkommen tilbage!</h3>
                </div>
                <div class="modal-body" style="padding-top: 30px;">

                    <form asp-controller="account" asp-action="externalLogin" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post">
                        <button name="provider" value="Facebook" type="submit" class="btn btn-block btn-social btn-facebook btn-lg">
                            <span class="fa fa-facebook"></span> Log ind via Facebook
                        </button>
                        <button name="provider" type="submit" class="btn btn-block btn-social btn-google btn-lg" value="Google" style="margin-top: 20px;">
                            <span class="fa fa-google"></span> Log ind via Google
                        </button>
                    </form>

                    <div class="divider">
                        <h4><span>ELLER</span></h4>
                    </div>

                    <a id="emailLoginButton" class="btn btn-block btn-social btn-google btn-lg" style="background: #FFA500">
                        <span class="fa fa-envelope fa"></span> Log ind via Email
                    </a>

                    <div id="emailLogin" style="display: none">
                        <div style="margin-top: 20px;"></div>
                        <span id="errorMsgTop" style="color: red;"></span>
                        @Html.AntiForgeryToken()
                        <input id="email" type="email" class="text-input" placeholder="Email" />
                        <span id="errorMsgBot" style="color: red"></span>
                        <input id="password" class="text-input" placeholder="Kodeord" type="password" />
                        <input type="button" class="login-button" id="loginBtn" value="Log på" />
                    </div>

                    <div style="width: 100%; margin-top: 15px" class="text-center">
                        <a asp-controller="account" asp-action="forgotPassword" style="margin-top: 20px; color: #000; text-decoration: none;">Har du glemt kodeord?</a>
                    </div>


                </div>
            </div>
        </div>
    </div>

    <!-- Modal Opret -->
    <div id="opretModal" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width: 400px;">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h3 class="modal-title text-center">Opret din profil</h3>
                </div>
                <div class="modal-body" style="padding-top: 30px; padding-bottom: 15px;">

                    <form asp-controller="account" asp-action="externalLogin" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post">
                        <button name="provider" value="Facebook" type="submit" class="btn btn-block btn-social btn-facebook btn-lg">
                            <span class="fa fa-facebook"></span> Tilmeld via Facebook
                        </button>
                        <button name="provider" type="submit" value="Google" class="btn btn-block btn-social btn-google btn-lg" style="margin-top: 20px;">
                            <span class="fa fa-google"></span> Tilmeld via Google
                        </button>
                    </form>

                    <div class="divider">
                        <h4><span>ELLER</span></h4>
                    </div>

                    <a asp-controller="account" asp-action="registerConfirmation" asp-route-returnurl="@ViewData["ReturnUrl"]" class="btn btn-block btn-social btn-google btn-lg" style="background: #FFA500"><span class="fa fa-envelope fa"></span> Tilmeld via Email</a>

                    <div style="width: 100%; margin-top: 15px" class="text-center">
                        <a asp-controller="Account" asp-action="Login" asp-route-returnurl="@ViewData["ReturnUrl"]" style="margin-top: 20px; color: #000; text-decoration: none;">Har du en profil? Log in her</a>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <div class="container-fluid" style="padding: 0;">

        @RenderBody()

        @Html.Partial("_FooterPartial")
    </div>

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
        @*<script src="~/js/site.js" asp-append-version="true"></script>*@
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-K+ctZQ+LL8q6tP7I94W+qzQsfRV2a+AfHIi9k8z8l9ggpc8X+Ytst4yBo/hH+8Fk">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
        @*<script src="~/js/site.min.js" asp-append-version="true"></script>*@
    </environment>

    <script src="~/js/bestjavascript.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap-wysiwyg/bootstrap-wysiwyg.js"></script>
    <script src="~/lib/progressbar.js/dist/progressbar.min.js"></script>

    @RenderSection("Scripts", required: false)
    @RenderSection("SearchStar", required: false)

</body>
</html>
