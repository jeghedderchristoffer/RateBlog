@model RateBlog.Models.AdminViewModels.SeFeedbackViewModel

<div class="navbar-allaround"></div>


<table class="table"
            <thread>
               <tr>
                   <th>
                       <p>Kvalitet</p>

                   </th>
                   <th>
                       <p>Opførsel</p>

                   </th>
                   <th>
                       <p>Interaktion</p>

                   </th>
                   <th>
                       <p>Troværdighed</p>

                   </th>
                   <th>
                       <p>Feedback</p>
                   </th>
                   <th>
                       <p>Svar</p>
                   </th>




               </tr>

            </thread>

            <tbody>

                @foreach (var m in Model.ListRating)
                {
                    <tr>
                        <td>
                            <p>@m.Kvalitet</p>
                        </td>
                        <td>
                            <p>@m.Opførsel</p>
                        </td>

                        <td>
                            <p>@m.Interaktion</p>
                        </td>

                        <td>
                            <p>@m.Troværdighed</p>
                        </td>
                        <td>
                            <p>@m.Feedback</p>
                        </td>
                        <td>
                            <p>@m.Answer</p>
                        </td>
                        <td>
                            <p>@m.Answer</p>
                        </td>

                        <td>
                            <a asp-action="RedigereFeedback" asp-route-id="@m.RatingId">Edit</a> |
                            <a asp-action="DeleteFeedback" asp-route-id="@m.RatingId">Delete</a>
                        </td>


                        <td></td>
                    </tr>
                }




            </tbody>

        </table>