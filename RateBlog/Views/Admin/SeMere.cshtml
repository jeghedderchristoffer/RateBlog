@model RateBlog.Models.InfluenterViewModels.IndexViewModel
@inject UserManager<ApplicationUser> UserManager
@{


@{
    ViewData["Title"] = "Se mere om bruger";
}


<div class="navbar-allaround">
            div class="container">
    <table class="table table-striped">
        <thread>
            <tr>
                <th>
                    <p>navn</p>

                </th>
                <th>
                    <p>email</p>

                </th>
                <th>
                    <p>Influenter</p>

                </th>
                <th>
                    <p>
                        Se mere
                    </p>

                </th>
            </tr>
        </thread>
        <tbody>

            @foreach (var user in Model.InfluentList)
            {
                <tr>

                    <td>
                        <p>@user.Name</p>
                    </td>
                    <td>
                        <p>@user.Email</p>
                    </td>

                    <td>
                        <p>@user.InfluenterId</p>
                    </td>

                    <td>
                        <a class="col-md-offset-3 col-md-6 col-xs-12">
                            <input type="submit" asp-action="SeMere" style="margin-bottom: 20px" class="edit-button" value="Se mere" />

                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>

    </div>
